<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id = "com" class = "com.capgemini.in.SetterSpringUsingObject.Company">
		<property name="com_name" value = "Capgemini"></property>
        <property name="com_id" value = "CG"></property>
 </bean>
<bean id = "emp" class = "com.capgemini.in.SetterSpringUsingObject.Employee">
		<property name="ename" value = "Pradeep"></property>
        <property name="eid" value = "CG4554"></property>
        <property name="sal" value = "56754.43"></property>
        <property name="com" ref = "com"></property>
</bean>

</beans>